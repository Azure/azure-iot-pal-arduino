[thingdev_remote_monitoring]
Target=remote_monitoring.ino
SourcePath=!kits_root!\iot-hub-c-thingdev-getstartedkit\remote_monitoring
RelativeWorkingDir=\thingdev_remote_monitoring
CloneURL=https://github.com/Azure-Samples/iot-hub-c-thingdev-getstartedkit.git
MaxAllowedDurationSeconds=100
LogLines=300
MinimumHeap=7000
Categories=IoTHub
Hardware=SparkFun_ESP8266_Thing 
SerialPort=COM7
CPUParameters=-fqbn=esp8266:esp8266:thing:CpuFrequency=80,UploadSpeed=115200 -vid-pid=0X239A_0X800B -ide-version=10609
Build=Enable
Execution=Enable

